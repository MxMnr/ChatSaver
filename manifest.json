{
  "manifest_version": 2,
  "name": "Save Chat",
  "description": "This extension saves chat data from OpenAI chat",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "https://chat.openai.com/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
